{
  "name": "pdf-server",
  "private": true,
  "version": "3.0.0-rc.3",
  "description": "PDF Server for form.io",
  "main": "index.js",
  "watch": {
    "start": {
      "patterns": [
        "src",
        "node_modules/formio-pdf/dist"
      ],
      "ignore": "src/lib",
      "extensions": "js",
      "quiet": false
    }
  },
  "scripts": {
    "start": "node index.js",
    "clean": "rm -rf build",
    "compile": "node compile",
    "build": "npm run lint; npm run clean; npm run compile",
    "watch": "npm-watch",
    "lint": "eslint . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@formio/pdf": "github:formio/formio-pdf#v2.0.0-rc.9",
    "async": "^3.2.0",
    "aws-sdk": "^2.726.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "chance": "^1.1.5",
    "core-js": "3.5.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "errdrop": "^0.2.0",
    "express": "^4.17.1",
    "formio-service": "^1.4.0",
    "formio-viewer": "2.48.0-rc.6",
    "formiojs": "4.11.1-rc.9",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "minio": "^7.0.15",
    "mongodb": "^3.5.9",
    "multer": "^1.4.2",
    "node-cache": "^5.1.1",
    "puppeteer": "^5.2.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "del": "^5.1.0",
    "eslint": "^7.3.1",
    "eslint-config-formio": "^1.1.4",
    "gulp": "^4.0.2",
    "gulp-watch": "^5.0.1",
    "mocha": "^8.0.1",
    "npm-watch": "^0.6.0",
    "pre-commit": "^1.2.2",
    "webpack": "^4.41.5",
    "webpack-obfuscator": "^1.2.2"
  },
  "pre-commit": [
    "lint"
  ]
}

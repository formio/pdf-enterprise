{
  "name": "pdf-server",
  "private": true,
  "version": "3.0.0",
  "description": "PDF Server for form.io",
  "main": "index.js",
  "watch": {
    "start": {
      "patterns": [
        "src",
        "node_modules/formio-pdf/dist"
      ],
      "ignore": "src/lib",
      "extensions": "js",
      "quiet": false
    }
  },
  "pre-commit": [
    "lint",
    "lib"
  ],
  "scripts": {
    "start": "node index.js",
    "clean": "rm -rf build",
    "compile": "node compile",
    "lib": "gulp build",
    "build": "npm run lib; npm run lint; npm run clean; npm run compile",
    "watch": "npm-watch",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.12.6",
    "async": "^3.2.0",
    "aws-sdk": "^2.829.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "chance": "^1.1.7",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "errdrop": "^0.2.0",
    "eslint": "^7.18.0",
    "eslint-config-formio": "^1.1.4",
    "express": "^4.17.1",
    "formio-service": "^1.4.0",
    "formio-viewer": "^2.50.2",
    "formiojs": "^4.12.7",
    "fs-extra": "^9.1.0",
    "ignore": "^5.1.8",
    "jose": "^2.0.3",
    "lodash": "^4.17.20",
    "minio": "^7.0.18",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "node-cache": "^5.1.1",
    "puppeteer": "^5.5.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@formio/pdf": "github:formio/formio-pdf#v2.0.2",
    "@formio/premium": "github:formio/premium#v1.15.4",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-watch": "^5.0.1",
    "javascript-obfuscator": "^2.10.2",
    "mocha": "^8.2.1",
    "npm-watch": "^0.7.0",
    "pre-commit": "^1.2.2",
    "webpack": "^5.16.0",
    "webpack-obfuscator": "^3.2.0"
  }
}
